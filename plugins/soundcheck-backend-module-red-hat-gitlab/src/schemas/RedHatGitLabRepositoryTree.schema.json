{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "RedHatGitLabRepositoryTree",
  "title": "Repository tree",
  "description": "A project's repository tree.",
  "type": "object",
  "properties": {
    "tree": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "mode": {
            "type": "string"
          }
        }
      }
    }
  }
}
