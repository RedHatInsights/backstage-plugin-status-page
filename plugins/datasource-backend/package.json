  "devDependencies": {
    "@backstage/backend-test-utils": "^1.9.0",
    "@backstage/cli": "^0.34.1",
    "@backstage/repo-tools": "^0.15.3",
    "@types/express": "^4.17.6",
    "@types/luxon": "^3",
    "@types/supertest": "^2.0.12",
    "sqlite3": "^5.1.7",
    "supertest": "^6.2.4"
